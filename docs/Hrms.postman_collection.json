      "name": "AuthenticateViewer",
      "event": [
        {
          "listen": "test",
          "script": {
            "exec": [
              "// Parse the JSON response\r",
              "var jsonData = pm.response.json();\r",
              "\r",
              "// Extract the token (adjust 'access_token' to match your API's key name)\r",
              "var token = jsonData.access_token;\r",
              "\r",
              "// Save to a Global variable\r",
              "pm.globals.set(\"api_access_token\", token);\r",
              "\r",
              "console.log(\"Token has been saved to globals!\");"
